<div class="span6 offset1">
	<table>

	<h2>Top Content</h2>
	<table class="table">
		<tr>
			<th>Page title</th>
			<th>Pageviews</th>
		</tr>
		<% @pages.each do |page| %>
		<tr>
			<td><%= page.page_title %></td>
			<td><%= page.pageviews %></td>
		</tr>
		<% end %>
	</table>


		<h2>All profiles</h2>
		<% @profiles.each do |profile| %>
		<%= profile.title %>: <%= profile.id %><br>
		<% end %>

		<h2>All Web Property ID</h2>
		<% @web_properties.each do |property| %>
		<%= property.account_id %>: <%= property.id %><br>
		<% end %>

	</table>
</div>