<!-- "#{session[:user_email]}" || "#{current_user.email}" -->
<% if current_user %>
  <script type="text/javascript">
    analytics.identify(<%= current_user.id %>,
      <%= ( current_user.identify_properties.html_safe ) %>)
  </script>
<% end %>

<% if flash[:track] && flash[:track].kind_of?(Array)%>
    <% flash[:track].each do |event_name| %>
    <script type="text/javascript">
      analytics.track("<%= event_name %>", {});
    </script>
  <% end %>
<% end %>

